@model Book

<!--partial view to display the book summaries on the index.cshtml page-->
<tr>
    <td>@Model.Title</td>
    <td>@Model.AuthorLastName</td>
    <td>@Model.AuthorFirstName</td>
    <td>@Model.AuthorMiddleInitial</td>
    <td>@Model.Publisher</td>
    <td>@Model.ISBN</td>
    <td>@Model.Classification</td>
    <td>@Model.Category</td>
    <td>$@Model.Price</td>
    <td>@Model.PageNum</td>
</tr>